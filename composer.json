{
    "name": "jupi/dropzonejs-uploader-bundle",
    "description": "Symfony bundle to upload files with Dropzone.js",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jupi007",
            "email": "paul.kepinski@orange.fr"
        }
    ],
    "require": {
        "psr/log": "^1|^2|^3",
        "symfony/filesystem": "^6.2",
        "symfony/config": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/http-foundation": "^6.2",
        "symfony/http-kernel": "^6.2"
    },
    "require-dev": {
        "symfony/framework-bundle": "^6.2",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Jupi\\DropzoneJsUploaderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jupi\\DropzoneJsUploaderBundle\\Tests\\": "tests/"
        }
    }
}
